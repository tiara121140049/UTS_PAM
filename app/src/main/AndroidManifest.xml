<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/splash_ikon"
        android:label="@string/aplikasi"
        android:roundIcon="@mipmap/splash_ikon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="31">
        <activity
            android:name=".homepage.detail"
            android:exported="false" />
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:launchMode="singleTop"
            android:noHistory="true"
            android:theme="@style/Theme.RemoveSplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.Login"
            android:exported="false"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".login.Register"
            android:exported="false"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".homepage.MainActivity"
            android:exported="false"
            android:theme="@style/LoginTheme"></activity>
        <activity
            android:name=".homepage.Profile"
            android:exported="false"
            android:theme="@style/LoginTheme" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>